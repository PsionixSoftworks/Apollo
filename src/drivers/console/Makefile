# Required for any Console
obj_$(CONFIG_CONSOLE) += kernel_console.c

# Hosted local console
obj_$(CONFIG_CONSOLE_HOSTED) += hosted_console.c

# Basic VGA Console
obj_$(CONFIG_CONSOLE_VGA) += vga_console.c
